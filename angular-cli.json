{
  "project": {
    "version": "1.0.0-beta.26",
    "name": "portalqimobadmin"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "../src/assets/vendors/bower_components/animate.css/animate.min.css",
        "../src/assets/vendors/bower_components/bootstrap-select/dist/css/bootstrap-select.css",
        "../src/assets/vendors/bower_components/sweetalert2/dist/sweetalert2.min.css",
        "../src/assets/vendors/bower_components/fullcalendar/dist/fullcalendar.min.css",
        "../src/assets/vendors/bower_components/material-design-iconic-font/dist/css/material-design-iconic-font.min.css",
        "../src/assets/vendors/bower_components/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css",
        "../src/assets/css/app_1.min.css",
        "../src/assets/css/app_2.min.css",
        "../src/assets/css/primeng/theme.css",
        "../node_modules/primeng/resources/primeng.css",
        "../node_modules/font-awesome/css/font-awesome.css"
      ],
      "scripts": [
        "../src/assets/vendors/bower_components/jquery/dist/jquery.min.js",
        "../src/assets/vendors/bower_components/bootstrap/dist/js/bootstrap.min.js",

        "../src/assets/vendors/sparklines/jquery.sparkline.min.js",
        "../src/assets/vendors/bower_components/flot/jquery.flot.js",
        "../src/assets/vendors/bower_components/flot/jquery.flot.resize.js",
        "../src/assets/vendors/bower_components/flot.curvedlines/curvedLines.js",
        "../src/assets/vendors/bower_components/jquery.easy-pie-chart/dist/jquery.easypiechart.min.js",

        "../src/assets/vendors/bootstrap-growl/bootstrap-growl.min.js",
        "../src/assets/vendors/bower_components/moment/min/moment.min.js",
        "../src/assets/vendors/bower_components/bootstrap-select/dist/js/bootstrap-select.js",
        "../src/assets/vendors/bower_components/fullcalendar/dist/fullcalendar.min.js",
        "../src/assets/vendors/bower_components/simpleWeather/jquery.simpleWeather.min.js",
        "../src/assets/vendors/bower_components/waves/dist/waves.min.js",
        "../src/assets/vendors/bower_components/sweetalert2/dist/sweetalert2.min.js",
        "../src/assets/vendors/bower_components/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js",
        "../src/assets/js/app.min.js",
        "../src/assets/js/pagseguro/sandbox/pagseguro.directpayment.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
